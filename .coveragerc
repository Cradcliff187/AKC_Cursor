[run]
source = app
omit =
    app/__pycache__/*
    app/static/*
    app/*/test_*.py
    tests/*
    venv/*
    env/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    def main
    if TYPE_CHECKING:
    
[html]
directory = htmlcov 